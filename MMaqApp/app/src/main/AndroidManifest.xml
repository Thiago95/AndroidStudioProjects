<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.theagobueno.mmaqapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="MMaqsApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitys.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activitys.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Controller.CadastroMaquinarioActivity" />
        <activity android:name=".Activitys.ActvtTabMaquinario" />
        <activity android:name=".Activitys.ActvtTabFuncionario" />
        <activity android:name=".Activitys.ActvtTabRelatorio" />
    </application>

</manifest>